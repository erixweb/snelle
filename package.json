{
  "name": "snelle",
  "author": "erixweb",
  "description": "A simple static site generator",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bun run dev.ts",
    "build": "bun run build.ts",
    "create": "bun run create.ts"
