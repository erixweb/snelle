{
	"name": "snelle",
	"author": "erixweb",
	"description": "A simple static site generator",
	"version": "0.0.9",
	"type": "module",
	"scripts": {
		"dev": "cli.js",
		"build": "bun run build.js",
		"create": "bun run create.js"
	},
	"bin": "cli.js",	
	"files": [
		"components/*",
		"cli.js",
		"dev.js",
		"build.js",
		"create.js",
		"src/*",
		"public/*"
	],
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0"
	}
}
